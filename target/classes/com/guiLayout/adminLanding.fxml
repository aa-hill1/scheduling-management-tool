<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane fx:id="anchorPane" onMouseMoved="#setUpScene" prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.guiLayout.controllers.admin.AdminLandingController">
   <children>
      <Button fx:id="logoutButton" layoutX="299.0" layoutY="10.0" mnemonicParsing="false" onAction="#openLoginScene" text="Logout" />
      <GridPane layoutX="116.0" layoutY="70.0" prefHeight="111.0" prefWidth="169.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="200.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="adminViewButton" mnemonicParsing="false" onAction="#openAdminViewScene" text="View Shifts and Routes" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
            <Button fx:id="adminEditButton" mnemonicParsing="false" onAction="#openAdminEditScene" text="Edit Stored Data" GridPane.halignment="CENTER" />
         </children>
      </GridPane>
      <Label fx:id="currentAccount" layoutX="158.0" layoutY="14.0" text="CurrentAccount" />
   </children>
</AnchorPane>
